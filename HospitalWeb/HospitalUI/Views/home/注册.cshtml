                    <img src="./images/登录界面切片/密码.png" alt="">
                    <p>确认密码</p>
                    </div>
                    <input type="password" name="repassword">
                   <input type="checkbox">
                   <button type="submit" name="sub" id="sub">注册</button>
                   <div>
                       <p>已经注册？</p>
                       <a href="./登录.html">点击登录</a>
                   </div>
             </div>
            </form>
        </div>
